
@model IEnumerable<MainDMMM.Models.Entities.ImageSlider>


<div class="slider-wrapper theme-default">
    <div id="slider" class="nivoSlider">

        @foreach (var image in Model)
        {
            if (image.Content != null)
            {
              
                    <img data-u="image" src="data:image/jpg;base64,@(Convert.ToBase64String(image.Content))"  data-thumb="data:image/jpg;base64,@(Convert.ToBase64String(image.Content))" alt="dmmmc" />

           
            }
        }
    </div>

</div>

@**@